{
	"name": "@rbxts/hotfusion",
	"version": "0.3.0-hotfusion.1",
	"description": "Fork of Fusion with improvements used by Team Fireworks",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/team-fireworks/hotfusion.git"
	},
	"contributors": [
		"dphfox",
		"znotfireman"
	],
	"keywords": [
